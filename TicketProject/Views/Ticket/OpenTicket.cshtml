<section class="content-header">
    <h1>
        Manage Ticket
        <small>19 October 2016</small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
        <li class="active">Dashboard</li>
    </ol>
</section>
<div ng-controller="OpenTicketController">
    <!-- SELECT2 EXAMPLE -->
    <div class="col-md-12">
        <!-- Horizontal Form -->
        <div class="box box-primary">
            <div class="box-header with-border">
                <h3 class="box-title">Open Ticket</h3>
            </div>
            <!-- /.box-header -->
            <!-- form start -->
            <form class="form-horizontal">
                <div class="box-body">
                    <div class="form-group">
                        <label for="inputEmail3" class="col-sm-3 control-label">Merchant Name <span style="color:red">*</span> : </label>
                        <div class="col-sm-8">
                            <div custom-select="f for f in fruits| filter: $searchTerm" ng-model="fruit">
                            </div>
                        </div>
                    </div>

                    <div class="form-group">
                        <label for="inputPassword3" class="col-sm-3 control-label">Brand Name <span style="color:red">*</span> : </label>
                        <div class="col-sm-8">
                            <div custom-select="f for f in fruits| filter: $searchTerm" ng-model="fruit">
                            </div>
                        </div>
                    </div>

                    <div class="form-group">
                        <label for="inputPassword3" class="col-sm-3 control-label">Shop Name  <span style="color:red">*</span> : </label>
                        <div class="col-sm-8">
                            <div custom-select="f for f in fruits| filter: $searchTerm" ng-model="fruit">
                            </div>
                        </div>
                    </div>

                    <div class="form-group">
                        <label for="inputPassword3" class="col-sm-3 control-label">Contact Name  <span style="color:red">*</span> : </label>
                        <div class="col-sm-3">
                            <input type="email" class="form-control" id="inputEmail3" placeholder="Email">
                        </div>
                    </div>

                    <div class="form-group">
                        <label for="inputPassword3" class="col-sm-3 control-label">Mobile : </label>
                        <div class="col-sm-3">
                            <input type="email" class="form-control" id="inputEmail3" placeholder="Email">
                        </div>
                    </div>

                    <div class="form-group">
                        <label for="inputPassword3" class="col-sm-3 control-label">Mobile : </label>
                        <div class="col-sm-3">
                            <input type="email" class="form-control" id="inputEmail3" placeholder="Email">
                        </div>
                    </div>

                    <div class="form-group">
                        <label for="inputPassword3" class="col-sm-3 control-label">Ticket Group : </label>
                        <div class="col-sm-8">
                            <div custom-select="f for f in fruits| filter: $searchTerm" ng-model="fruit">
                            </div>
                        </div>
                    </div>

                    <div class="form-group">
                        <label for="inputPassword3" class="col-sm-3 control-label">Ticket Subject <span style="color:red">*</span> :  </label>
                        <div class="col-sm-3">
                            <input type="email" class="form-control" id="inputEmail3" placeholder="Email">
                        </div>
                    </div>

                    <summernote height="300"></summernote>
                </div>
                <!-- /.box-footer -->
            </form>
        </div>
        <!-- /.box -->
    </div>
</div>